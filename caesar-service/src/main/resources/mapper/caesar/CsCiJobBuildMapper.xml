  <select id="queryCiJobBuildGroupByHour"
          resultMap="BuildTaskGroupByHourMap">
    select DATE_FORMAT(create_time,'%H') hours,count(id) count
       from cs_ci_job_build
       group by hours
       order by hours;
  </select>


